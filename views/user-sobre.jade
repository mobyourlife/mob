extends layout

block content
    //- Jumbotron
    div.jumbotron
        div.container
            h1 Sobre
    
    div.container.space-rows
        each i in info
            if i.value
                div.row
                    div.col-sm-3
                        p.pull-right
                            strong #{i.key}:
                    div.col-sm-9
                        | #{i.value}
                        if i.list
                            ul
                            each l in i.list
                                li #{l.name}